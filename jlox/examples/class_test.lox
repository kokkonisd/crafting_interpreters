class Heyyo {
    sayHi() {
        if (this.message) {
            return this.message;
        } else {
            return "hiya";
        }
    }

    getHiCb() {
        return this.sayHi;
    }
}

var myheyyo = Heyyo();
print Heyyo;
print myheyyo;

